    </m:msup>
    <m:msup>
     <m:mi>n</m:mi>
     <m:mrow>
      <m:mn>−2</m:mn>
     </m:mrow>
    </m:msup>
    
   </m:mrow>
   <m:mrow>
    <m:msqrt>
     <m:mrow>
      <m:msup>
       <m:mi>m</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:msup>
       <m:mi>c</m:mi>
       <m:mn>4</m:mn>
      </m:msup>
      
     </m:mrow>
    </m:msqrt>
    
   </m:mrow>
  </m:mfrac>
  
 </m:mrow>
</m:math>
</para></problem>
<solution id="fs-id1167339438268"><para id="fs-id1167339438269"><m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:msqrt>
     <m:mrow>
      <m:mi>m</m:mi><m:mi>n</m:mi><m:mi>c</m:mi>
     </m:mrow>
    </m:msqrt>
    
   </m:mrow>
   <m:mrow>
    <m:msup>
     <m:mi>a</m:mi>
     <m:mn>9</m:mn>
    </m:msup>
    <m:mi>c</m:mi><m:mi>m</m:mi><m:mi>n</m:mi>
   </m:mrow>
  </m:mfrac>
  
 </m:mrow>
</m:math>
</para></solution></exercise>
<exercise id="fs-id1167339438318"><problem id="fs-id1167339438319"><para id="fs-id1167339438320"><m:math>
 <m:mrow>
  <m:mfrac>
   <m:mi>a</m:mi>
   <m:mrow>
    <m:mi>a</m:mi><m:mo>−</m:mo><m:msqrt>
     <m:mi>c</m:mi>
    </m:msqrt>
    
   </m:mrow>
  </m:mfrac>
  
 </m:mrow>
</m:math>
</para></problem></exercise>
<exercise id="fs-id1167339438349"><problem id="fs-id1167339438350"><para id="fs-id1167339438351"><m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:mi>x</m:mi><m:msqrt>
     <m:mrow>
      <m:mn>64</m:mn><m:mi>y</m:mi>
     </m:mrow>
    </m:msqrt>
    <m:mo>+</m:mo><m:mn>4</m:mn><m:msqrt>
     <m:mi>y</m:mi>
    </m:msqrt>
    
   </m:mrow>
   <m:mrow>
    <m:msqrt>
     <m:mrow>
      <m:mn>128</m:mn><m:mi>y</m:mi>
     </m:mrow>
    </m:msqrt>
    
   </m:mrow>
  </m:mfrac>
  
 </m:mrow>
</m:math>
</para></problem>
<solution id="fs-id1167339438405"><para id="fs-id1167339438406"><m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:mn>2</m:mn><m:msqrt>
     <m:mn>2</m:mn>
    </m:msqrt>
    <m:mi>x</m:mi><m:mo>+</m:mo><m:msqrt>
     <m:mn>2</m:mn>
    </m:msqrt>
    
   </m:mrow>
   <m:mn>4</m:mn>
  </m:mfrac>
  
 </m:mrow>
</m:math>
</para></solution></exercise>
<exercise id="fs-id1167339438444"><problem id="fs-id1167339438445"><para id="fs-id1167339438446"><m:math>
 <m:mrow>
  <m:mrow><m:mo>(</m:mo>
   <m:mrow>
    <m:mfrac>
     <m:mrow>
      <m:msqrt>
       <m:mrow>
        <m:mn>250</m:mn><m:msup>
         <m:mi>x</m:mi>
         <m:mn>2</m:mn>
        </m:msup>
        
       </m:mrow>
      </m:msqrt>
      
     </m:mrow>
     <m:mrow>
      <m:msqrt>
       <m:mrow>
        <m:mn>100</m:mn><m:msup>
         <m:mi>b</m:mi>
         <m:mn>3</m:mn>
        </m:msup>
        
       </m:mrow>
      </m:msqrt>
      
     </m:mrow>
    </m:mfrac>
    
   </m:mrow>
  <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
   <m:mrow>
    <m:mfrac>
     <m:mrow>
      <m:mn>7</m:mn><m:msqrt>
       <m:mi>b</m:mi>
      </m:msqrt>
      
     </m:mrow>
     <m:mrow>
      <m:msqrt>
       <m:mrow>
        <m:mn>125</m:mn><m:mi>x</m:mi>
       </m:mrow>
      </m:msqrt>
      
     </m:mrow>
    </m:mfrac>
    
   </m:mrow>
  <m:mo>)</m:mo></m:mrow>
 </m:mrow>
</m:math>
</para></problem></exercise>
<exercise id="fs-id1167339438550"><problem id="fs-id1167339438552"><para id="fs-id1167339438553"><m:math>
 <m:mrow>
  <m:msqrt>
   <m:mrow>
    <m:mfrac>
     <m:mrow>
      <m:mroot>
       <m:mrow>
        <m:mn>64</m:mn>
       </m:mrow>
       <m:mn>3</m:mn>
      </m:mroot>
      <m:mo>+</m:mo><m:mroot>
       <m:mrow>
        <m:mn>256</m:mn>
       </m:mrow>
       <m:mn>4</m:mn>
      </m:mroot>
      
     </m:mrow>
     <m:mrow>
      <m:msqrt>
       <m:mrow>
        <m:mn>64</m:mn>
       </m:mrow>
      </m:msqrt>
      <m:mo>+</m:mo><m:msqrt>
       <m:mrow>
        <m:mn>256</m:mn>
       </m:mrow>
      </m:msqrt>
      
     </m:mrow>
    </m:mfrac>
    
   </m:mrow>
  </m:msqrt>
  
 </m:mrow>
</m:math>
</para></problem>
<solution id="fs-id1167339438628"><para id="fs-id1167339438629"><m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:msqrt>
     <m:mn>3</m:mn>
    </m:msqrt>
    
   </m:mrow>
   <m:mn>3</m:mn>
  </m:mfrac>
  
 </m:mrow>
</m:math>
</para></solution></exercise>

