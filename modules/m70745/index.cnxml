</solution>
</exercise>
<exercise id="fs-id2172406">
<problem id="fs-id1094601">
<para id="fs-id2216272"><m:math><m:mrow><m:mo>−</m:mo><m:mfrac><m:mrow><m:mn>4</m:mn><m:mi>x</m:mi></m:mrow><m:mrow><m:mn>32</m:mn><m:mi>y</m:mi></m:mrow></m:mfrac></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1594743">
<problem id="fs-id2288018">
<para id="fs-id1357548"><m:math><m:mrow><m:mfrac><m:mrow><m:mn>14</m:mn><m:msup><m:mi>x</m:mi><m:mn>2</m:mn></m:msup></m:mrow><m:mrow><m:mn>21</m:mn><m:mi>y</m:mi></m:mrow></m:mfrac></m:mrow></m:math></para>
</problem>
<solution id="fs-id1727185">
