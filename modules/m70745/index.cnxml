</solution>
</exercise>
<exercise id="fs-id1547777">
<problem id="fs-id2369338">
<para id="fs-id1033577"><m:math><m:mrow><m:mo>−</m:mo><m:mfrac><m:mrow><m:mn>63</m:mn></m:mrow><m:mrow><m:mn>99</m:mn></m:mrow></m:mfrac></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1032604">
<problem id="fs-id1788807">
<para id="fs-id1603310"><m:math><m:mrow><m:mo>−</m:mo><m:mfrac><m:mrow><m:mn>108</m:mn></m:mrow><m:mrow><m:mn>63</m:mn></m:mrow></m:mfrac></m:mrow></m:math></para>
</problem>
<solution id="fs-id2325399">
<para id="fs-id2817966"><m:math><m:mrow><m:mo>−</m:mo><m:mfrac><m:mrow><m:mn>12</m:mn></m:mrow><m:mn>7</m:mn></m:mfrac></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id2793119">
<problem id="fs-id574581">
<para id="fs-id2261318"><m:math><m:mrow><m:mo>−</m:mo><m:mfrac><m:mrow><m:mn>104</m:mn></m:mrow><m:mrow><m:mn>48</m:mn></m:mrow></m:mfrac></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1684083">
<problem id="fs-id1381828">
<para id="fs-id1363951"><m:math><m:mrow><m:mfrac><m:mrow><m:mn>120</m:mn></m:mrow><m:mrow><m:mn>252</m:mn></m:mrow></m:mfrac></m:mrow></m:math></para>
</problem>
<solution id="fs-id2196239">
<para id="fs-id1970881"><m:math><m:mrow><m:mfrac><m:mrow><m:mn>10</m:mn></m:mrow><m:mrow><m:mn>21</m:mn></m:mrow></m:mfrac></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id1741803">
<problem id="fs-id2280962">
<para id="fs-id1967034"><m:math><m:mrow><m:mfrac><m:mrow><m:mn>182</m:mn></m:mrow><m:mrow><m:mn>294</m:mn></m:mrow></m:mfrac></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1238655">
<problem id="fs-id2655499">
<para id="fs-id1406695"><m:math><m:mrow><m:mo>−</m:mo><m:mfrac><m:mrow><m:mn>168</m:mn></m:mrow><m:mrow><m:mn>192</m:mn></m:mrow></m:mfrac></m:mrow></m:math></para>
</problem>
<solution id="fs-id2687031">
<para id="fs-id1799726"><m:math><m:mrow><m:mo>−</m:mo><m:mfrac><m:mn>7</m:mn><m:mn>8</m:mn></m:mfrac></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id2903548">
<problem id="fs-id2262648">
<para id="fs-id1607745"><m:math><m:mrow><m:mo>−</m:mo><m:mfrac><m:mrow><m:mn>140</m:mn></m:mrow><m:mrow><m:mn>224</m:mn></m:mrow></m:mfrac></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id2279769">
<problem id="fs-id2676746">
<para id="fs-id1302203"><m:math><m:mrow><m:mfrac><m:mrow><m:mn>11</m:mn><m:mi>x</m:mi></m:mrow><m:mrow><m:mn>11</m:mn><m:mi>y</m:mi></m:mrow></m:mfrac></m:mrow></m:math></para>
</problem>
<solution id="fs-id1402050">
<para id="fs-id1899705"><m:math><m:mrow><m:mfrac><m:mi>x</m:mi><m:mi>y</m:mi></m:mfrac></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id2270719">
<problem id="fs-id2338434">
<para id="fs-id4153464"><m:math><m:mrow><m:mfrac><m:mrow><m:mn>15</m:mn><m:mi>a</m:mi></m:mrow><m:mrow><m:mn>15</m:mn><m:mi>b</m:mi></m:mrow></m:mfrac></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id2716617">
<problem id="fs-id908852">
<para id="fs-id1960210"><m:math><m:mrow><m:mo>−</m:mo><m:mfrac><m:mrow><m:mn>3</m:mn><m:mi>x</m:mi></m:mrow><m:mrow><m:mn>12</m:mn><m:mi>y</m:mi></m:mrow></m:mfrac></m:mrow></m:math></para>
</problem>
<solution id="fs-id3203073">
