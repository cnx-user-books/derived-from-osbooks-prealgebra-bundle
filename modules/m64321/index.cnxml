</m:math>
</para>
</problem> 
<solution id="fs-id1165135541804">
<para id="fs-id1165135541807"><m:math>
<m:mrow>
<m:msup>
<m:mi>f</m:mi>
<m:mrow>
<m:mo>−</m:mo><m:mn>1</m:mn></m:mrow>
</m:msup>
<m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:msup>
<m:mrow>
<m:mo stretchy="false">(</m:mo><m:mn>1</m:mn><m:mo>+</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo></m:mrow>
<m:mrow>
<m:mn>1</m:mn><m:mo>/</m:mo><m:mn>3</m:mn></m:mrow>
