<m:mi>f</m:mi>
<m:mrow>
<m:mo>−</m:mo><m:mn>1</m:mn></m:mrow>
</m:msup>
<m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:msqrt>
<m:mi>x</m:mi>
</m:msqrt>
<m:mo>−</m:mo><m:mn>7</m:mn></m:mrow>
